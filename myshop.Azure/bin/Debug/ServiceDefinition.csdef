        <SourceDirectory path="C:\Users\user\Desktop\myshop\myshop_final\myshop.Azure\ecf\myshopContent\" />
      </Content>
    </Contents>
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
